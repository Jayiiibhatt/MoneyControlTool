<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
   PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
    <class name="Model.Login" table="user">
        <id name="uname" coloumn="user_id" type="java.lang.String">
            <generator class="assigned"/>
        </id>
        <property name="pass" coloumn="password" type="java.lang.String"/> 
    </class>
</hibernate-mapping>